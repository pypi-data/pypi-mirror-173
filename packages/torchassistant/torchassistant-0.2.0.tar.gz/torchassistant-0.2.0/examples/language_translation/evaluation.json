{
    "session_dir": "pretrained",
    "initialize": {
        "definitions": {
            "computation": {
                "replace_strategy": "override",
                "override_key": [
                    "name"
                ],
                "options": [
                    {
                        "group": "metrics",
                        "name": "WER",
                        "spec": {
                            "class": "WordErrorRate",
                            "inputs": ["y_hat", "input_2"],
                            "transform": "torchassistant.transforms.DecodeText"
                        }
                    }
                ]
            }
        },
        "pipelines": {
            "train": {
                "input_injector": [
                    {
                        "data_loader": "autogenerated_data_loader_for_autogenerated_autogenerated_split.train"
                    }],
                "losses": [
                    {
                        "node_name": "neural_translator",
                        "loss_name": "cross_entropy",
                        "loss_display_name": "ed_loss"
                    }
                ],
                "metrics": [
                    {
                        "node_name": "neural_translator",
                        "metric_name": "WER",
                        "display_name": "WER"
                    }
                ]
            },
            "evaluate": {
                "input_injector": [{
                    "data_loader": "autogenerated_data_loader_for_autogenerated_autogenerated_split.val"
                }],
                "losses": [
                    {
                        "node_name": "neural_translator",
                        "loss_name": "cross_entropy",
                        "loss_display_name": "val ed_loss"
                    }
                ],
                "metrics": [
                    {
                        "node_name": "neural_translator",
                        "metric_name": "WER",
                        "display_name": "val WER"
                    }
                ]
            }
        }
    },
    "validation_pipelines": ["train", "evaluate"]
}
